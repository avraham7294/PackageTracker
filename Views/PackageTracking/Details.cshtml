@model PackageTracker.Models.PackageTracking

<h1>Package Details</h1>
<ul>
    <li><strong>Tracking Number:</strong> @Model.TrackingNumber</li>
    <li><strong>Carrier:</strong> @Model.Carrier</li>
    <li><strong>Status:</strong> @Model.Status</li>
    <li><strong>Shipping Date:</strong> @Model.ShippingDate.ToShortDateString()</li>
    <li><strong>Estimated Delivery Date:</strong> @Model.DeliveryDate.ToShortDateString()</li>
    <li><strong>Origin:</strong> @Model.Origin</li>
    <li><strong>Destination:</strong> @Model.Destination</li>
</ul>
<a href="/PackageTracking/Index">Track Another Package</a>
